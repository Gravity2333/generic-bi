    "@umijs/test": "^3.5.19",
    "cronstrue": "^2.2.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.4.1",
    "lint-staged": "^10.0.7",
    "prettier": "^2.2.0",
    "typescript": "^4.5.4",
    "webpack": "4.5.0",
    "yorkie": "^2.0.0"
