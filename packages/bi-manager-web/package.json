{
  "name": "@bi/manager-web",
  "version": "1.0.0",
  "scripts": {
    "start": "cross-env NODE_OPTIONS=--openssl-legacy-provider WEBPACK_FS_CACHE=none umi dev",
    "build": "cross-env NODE_OPTIONS=--openssl-legacy-provider WEBPACK_FS_CACHE=none umi build ",
    "postinstall": "umi generate tmp",
    "prettier": "prettier --write '**/*.{js,jsx,tsx,ts,less,md,json}'",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "@ant-design/icons": "^4.7.0",
    "@ant-design/pro-layout": "^6.34.6",
    "@ant-design/pro-table": "^2.68.3",
