    "@types/dom-to-image": "^2.6.4",
    "@types/lodash": "^4.14.175",
    "@types/react": "^17.0.0",
    "@types/react-copy-to-clipboard": "^5.0.2",
    "@types/react-dom": "^17.0.0",
    "@types/react-grid-layout": "^1.1.2",
    "@types/styled-components": "^5.1.14",
    "@types/uuid": "^8.3.3",
    "@umijs/fabric": "^2.8.1",
